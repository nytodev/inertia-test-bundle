{
    "name": "nytodev/inertia-symfony",
    "description": "The Symfony adapter for Inertia.js",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Tony Blard",
            "email": "tonyblard55@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nytodev\\InertiaSymfony\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nytodev\\InertiaSymfony\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "symfony/config": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/http-kernel": "^6.4",
        "symfony/twig-bundle": "^6.4",
        "twig/extra-bundle": "^3.20",
        "twig/twig": "^3.20",
        "symfony/http-client": "^6.4",
        "symfony/security-csrf": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 |^10.4|^11.5",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.72"
    }
}
